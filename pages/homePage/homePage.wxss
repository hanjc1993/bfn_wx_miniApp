}
.hide{
  display: none;
}
.show{
  display: block;
}
.show_images{
  background-color: white;
}
.top_top{
  position: relative;
}
.top_top .top_top_moreImg{
  position: absolute;
  bottom: 15rpx;
  right: 15rpx;
  width: 78rpx;
  height: 78rpx;
  border-radius: 10rpx;
  background-color: rgba(0,0,0,0.3);

}
.top_top .top_top_moreImg image{
  margin-top: 3rpx;
  margin-left: 21rpx;
  width: 40rpx;
  height: 40rpx;
  float: left;
}
.top_top .top_top_moreImg text{
  float: left;
  margin-left: 12rpx;
  font-size: 27rpx;
  color: #fff;
}
.swiper-box {
  height: 351rpx;
}
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{
  margin-bottom: 2rpx;
}
.swiper-box .wx-swiper-dot{
  width: 15rpx;
  height: 15rpx;
  display: inline-flex;
  margin-left: 20rpx;
  justify-content: space-between;
  
}
.swiper-box .wx-swiper-dot::before{
  display: inline-block;
  position: absolute;
  content: '';
  flex-grow: 1; 
  background: #ccc;
  /* height: 10rpx;
  width:10rpx; */
  border-radius: 50%
}
.swiper-box .wx-swiper-dot-active {
  background-color: #ffffff;
}
.swiper-box .wx-swiper-dot-active::before{
  display: inline-block;
  width: 26rpx;
  height: 15rpx;
  margin-left: -6rpx;/*挡在最中间..*/
  background:#ffffff;
  border-radius: 8rpx;
}
.slide-image {
  height: 100%;
  width: 100%;
}
.top_bottom{
  padding: 9rpx 17rpx;
}
.top_bottom_top{
  height: 60rpx;
  margin-bottom: 6rpx;
  overflow: hidden;
  position: relative;
}
.top_bottom_top .TBT_changeStore{
  float:left;
  margin: 9rpx 6rpx;
  width: 129rpx;
  padding: 6rpx 0;
  line-height: 28rpx;
  text-align: center;
  background-color: #ff796e;
  border-radius: 6rpx;
  box-shadow: 0 0 10rpx 3rpx rgba(0,0,0,0.15) inset,0 0 3rpx 0 rgba(0,0,0,0.3);
  font-size: 24rpx;
  color: #ffeeee;
}
.top_bottom_top .TBT_storeName{
  position: absolute;
  top: 0;
  left: 156rpx;
  right: 80rpx;
  line-height: 58rpx;
  font-size: 31rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.top_bottom_top .TBT_tel{
  float: right;
  padding: 0 15rpx 0 25rpx;
  border-left: 1px solid #eee;
  position: relative;
  width: 28rpx;
  height: 100%;
}
.top_bottom_top image{
  position: absolute;
  top:14rpx;
  width: 28rpx;
  height: 28rpx;
}
.top_bottom_bottom{
  overflow: hidden;
}
.top_bottom_bottom .TBB_viewCount{
  float: right;
  font-size: 20rpx;
  line-height: 30rpx;
  color: #bbb;
}
.top_bottom_bottom .TBB_viewCount image{
  margin-bottom: -3rpx;
  margin-right: 12rpx;
  width: 24rpx;
  height: 24rpx;
}

/**
门店详细信息
*/
.storeDetail{
  margin-top: 21rpx;
  background-color: white;
  padding: 26rpx 26rpx 23rpx 26rpx;
}
.storeDetail .storeDetail_Addr{
  font-size: 31rpx;
  line-height: 42rpx;
  position: relative;
}
.storeDetail_Addr .SDA_icon{
  position: absolute;
  top:0;
  height: 100%;
}
.storeDetail_Addr .SDA_icon image{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  width: 42rpx;
  height: 42rpx;
}
.storeDetail_Addr .SDA_left{
  left:0
}
.storeDetail_Addr .SDA_mid{
  margin: 0 68rpx;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.storeDetail_Addr .SDA_right{
  right: 45rpx
}

.storeDetail .storeDetail_Info{
  margin-top: 23rpx;
  padding-top: 24rpx;
  border-top: 1px solid #eee;
  overflow: hidden;
  letter-spacing: 1px;
}
.storeDetail_Info .SDI_item{
  line-height: 32rpx;
}
.storeDetail_Info .SDI_item image{
  float: left;
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}
.storeDetail_Info .SDI_item1{
  margin-bottom: 19rpx;
  float: left;
  font-size: 27rpx;
  color: #777;
}
.storeDetail_Info .SDI_openTime{
  margin-right: 20rpx;
  min-width: 50%;
}
.storeDetail_Info .SDI_item2{
  clear: left;
  font-size: 25rpx;
  color: #aaa;
}



