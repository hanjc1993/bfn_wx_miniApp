#notnull{
  padding-top: 10%;
  height:20rpx;
  width: 20rpx;
}
.show_project{
  position: fixed;
  bottom: 90rpx;
  width: 100%;
  background-color: #fff;
  text-align: center;
  height:700rpx;
}
.show_project_title{
  background-color: #f8f6f3;
  height: 90rpx;
  width: 100%;
  line-height: 90rpx
}
.show_project_title_left{
  float: left;
  width:20%;
  font-size: 35rpx
}
.show_project_title_mid{
  float: left;
  width: 60%
}
.show_project_title_right{
  float: right;
  width:20%;
  color:#fac281;
  font-size: 35rpx
}
.show_project_list{
  height: 200rpx;
  width: 95%;
  margin-left: 5%;

}
.show_project_row{
  border-bottom: 2rpx solid #e5e5e5;
  height: 200rpx;
  width: 100%;
}
.show_project_list_image{
  height: 200rpx;
  width: 30%;
  float: left;
}
.show_project_list_image image{
  padding-top: 8%;
  height:180rpx;
  width:100%
}
.show_project_list_text{
  float: left;
  margin-left: 3%;
  height:200rpx;
  width: 50%;
  text-align: left;
}
.show_project_list_text_top{
  font-size: 28rpx;
  margin-top: 15%
}
.show_project_list_text_bottom image{
  text-align: left;
  margin-top: 5%;
  width: 50%;
  height: 50rpx;
}
.choose_Project{
  float: right;
  width: 17%;
  text-align: center;
  height: 200rpx;
  line-height: 200rpx
}
.bottom_btn{
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: #cccccc;
  color:#fff;
  text-align: center;
  height:90rpx;
  line-height: 90rpx
}
.bottom_btn1{
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: #906337;
  color:#fff;
  text-align: center;
  height:90rpx;
  line-height: 90rpx
}
/* 单选钮样式 */
radio {
  transform:scale(0.6);
}
.addFriends{
  margin-top: 3%;
  font-size: 45rpx
}
.addFriends1{
  display:none
}
.scroll-view {
  width: 750rpx;
  white-space: nowrap;
  display: flex;
}
.navbar{
  flex: none;
  display: flex;
  background: #fff;
  width: 100%;
}
.navbar .item{
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 60rpx;
  font-size: 30rpx;
  margin-left: 70rpx;
  color: #999999;
}
.navbar .item.active{
  color: #a48b77;
  border-bottom:2rpx #a48b77 solid;
}
.timeLineView {
  width: 100%;
  text-align: center;
}
.timeLine {
  overflow: hidden;
  width: 90%;
  margin-left: 60rpx;
}
.time {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  float: left;
  margin-right: 35rpx;
  margin-bottom: 10rpx;
  font-size: 25rpx;
}
.mornimage {
  height: 30rpx;
  width: 30rpx;
  position: absolute;
  top: 15rpx;
  left: 60rpx;
}
.title{
  font-size: 12px;
  height: 60rpx;
  line-height: 60rpx;
  color:#999999;
  position: relative;
  padding-left: 100rpx;
}
/** dialog */

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.modal-dialog {
  width: 100%;
  height: 700rpx;
  overflow: hidden;
  position: fixed;
  bottom: 90rpx;
  background-color: #fff;
  text-align: center;
  z-index: 9999;
  background: #fff;
  
}
#stars{
  margin-top: 5%;
  width: 70%;
  height: 40rpx;
}
.showMsg{
  position: fixed;
  top:0;
  height:60rpx;
  width: 100%;
  background-color: #c9bc9c;
  font-size: 32rpx;
  text-align: center;
  color: white;
  z-index: 9999
}

/** end */