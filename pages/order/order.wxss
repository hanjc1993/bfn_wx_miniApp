  width: 100%;
  background-color: #fff;
  text-align: center;
  height: 700rpx;
}

.show_project_title {
  background-color: #f8f6f3;
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
}

.show_project_title_left {
  float: left;
  width: 20%;
