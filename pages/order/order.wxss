}

.tb_text {
  width: 57%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: left;
  border-bottom: 4rpx solid #e5e5e5;
  float: left;
  font-size: 32rpx;
  color: #333;
}

.rowName {
  width: 30%;
  height: 70rpx;
  line-height: 70rpx;
  float: left;
  font-size: 35rpx;
