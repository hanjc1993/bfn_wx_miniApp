  text-align: left;
}

.show_project_list_text_top {
  font-size: 30rpx;
  margin-top: 60rpx;
}

.show_project_list_text_bottom {
  text-align: left;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
}

.show_project_list_text_bottom text {
  background-color: #c9bc9c;
  color: white;
  border-radius: 8rpx;
}

.choose_Project {
  float: left;
  width: 10%;
  text-align: center;
  height: 200rpx;
  line-height: 200rpx;
}

.bottom_btn {
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: #ccc;
  color: #fff;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
}

.bottom_btn1 {
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: #906337;
  color: #fff;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
}

/* 单选钮样式 */

radio {
  transform: scale(0.6);
}

.addFriends {
  margin-top: 3%;
  font-size: 50rpx;
  height: 180rpx;
  color: #666;
}

.hideAddFriends {
  display: none;
}

.scroll-view {
  width: 750rpx;
  white-space: nowrap;
  display: flex;
}
/*隐藏滚动条*/
::-webkit-scrollbar {
  display: none
}

