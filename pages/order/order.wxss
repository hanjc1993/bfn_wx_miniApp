.timeLineView {
  width: 100%;
  text-align: center;
}

.timeLine {
  overflow: hidden;
  width: 90%;
  margin-left: 60rpx;
}

.time {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  float: left;
  margin-right: 35rpx;
  margin-bottom: 10rpx;
  font-size: 30rpx;
}

.fulltime {
  background-color: #ccc;
}

.mornimage {
  height: 40rpx;
  width: 40rpx;
  position: absolute;
  top: 12rpx;
  left: 60rpx;
}

.title {
  font-size: 15px;
  height: 60rpx;
  width: 60rpx;
  line-height: 60rpx;
  margin-bottom: 5rpx;
  color: #ccc;
  position: relative;
  padding-left: 110rpx;
}

/** dialog */

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-mask1 {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9900;
  color: #fff;
}

.modal-dialog {
  width: 100%;
  height: 800rpx;
  overflow: hidden;
  position: fixed;
  bottom: 0rpx;
  background-color: #fff;
  text-align: center;
  z-index: 9999;
  background: #fff;
}

#stars {
  margin-top: 5%;
  width: 70%;
  height: 40rpx;
}

/** end */

.arrow {
  float: right;
  margin-right: 30rpx;
  margin-top: 40rpx;
}

.arrowRotate {
  float: right;
  margin-right: 30rpx;
  margin-top: 40rpx;
  transform: rotate(90deg);
}

.content_text_right1 {
  float: left;
  height: 100%;
  width: 370rpx;
}

.content_text_right2 {
  height: 100%;
  margin-top: 13rpx;
  text-align: right;
  font-size: 35rpx;
  color: #b5b6b6;
}

.content_text_right2.orderRemark {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.other-button-hover {
  background-color: #906337;
}

/** 合并后加的 */

.chooseArea {
  height: 436rpx;
  width: 60%;
  overflow: hidden;
  position: fixed;
  bottom: 45%;
  margin-left: 20%;
  background-color: #fff;
  text-align: center;
  z-index: 9999;
  background: #faf8f7;
  border-radius: 10rpx;
}

.bind_phone {
  height: 570rpx;
  width: 70%;
  overflow: hidden;
  position: fixed;
  bottom: 38%;
  margin-left: 15%;
  background-color: #fff;
  text-align: center;
  z-index: 9800;
  background: #faf8f7;
  border-radius: 10rpx;
}

.bind_phone_top {
  width: 100%;
  height: 60rpx;
}

.bind_phone_top_btn {
  height: 60rpx;
  width: 20%;
  float: right;
}

.bindbtn {
  margin-top: 30rpx;
  width: 100%;
  background-color: #fbb059;
  color: #fff;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  border: none;
  border-top-left-radius: 0rpx;
  border-top-right-radius: 0rpx;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.bindbtn::after {
  border: none;
  border-top-left-radius: 0rpx;
  border-top-right-radius: 0rpx;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.bindbtnGray {
  margin-top: 30rpx;
  width: 100%;
  background-color: #ccc;
  color: white;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  border: none;
  border-top-left-radius: 0rpx;
  border-top-right-radius: 0rpx;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.bindbtnGray::after {
  border: none;
  border-top-left-radius: 0rpx;
  border-top-right-radius: 0rpx;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.saveArea {
  width: 100%;
  background-color: #fbb059;
  color: #fff;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
}

.tb {
  margin-top: 20rpx;
  width: 96%;
