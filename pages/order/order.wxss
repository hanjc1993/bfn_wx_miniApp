}

.show_project_row {
  border-bottom: 4rpx solid #e5e5e5;
  height: 200rpx;
  width: 100%;
}

.show_project_list_image {
  height: 200rpx;
  width: 30%;
  float: left;
}

.show_project_list_image image {
  padding-top: 25rpx;
  height: 150rpx;
  width: 220rpx;
}

.show_project_list_image_nurse {
  height: 200rpx;
  width: 30%;
  float: left;
}

.show_project_list_image_nurse image {
  padding-top: 25rpx;
  height: 150rpx;
  width: 150rpx;
}

.show_project_list_text {
  float: left;
  margin-left: 3%;
